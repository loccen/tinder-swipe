# 本地开发环境配置示例
# 复制此文件为 .env 用于本地开发

# =============================================================================
# Telegram 配置 (可选，不测试 Telegram 功能时可留空)
# =============================================================================
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_PHONE=
TELEGRAM_CHANNELS=[]
SESSION_PATH=../sessions/collector

# =============================================================================
# PikPak 配置 (可选)
# =============================================================================
PIKPAK_USERNAME=
PIKPAK_PASSWORD=

# =============================================================================
# Linode 配置 (可选，不测试云实例时可留空)
# =============================================================================
LINODE_TOKEN=
LINODE_REGION=ap-northeast
LINODE_TYPE=g6-nanode-1

# =============================================================================
# 代理配置 (可选)
# =============================================================================
SOCKS5_PORT=1080
SOCKS5_USERNAME=proxy
SOCKS5_PASSWORD=swipe2024

# =============================================================================
# Aria2 配置
# =============================================================================
# 本地开发可以指向远程 Aria2 或本地 Aria2
ARIA2_RPC_URL=http://localhost:6800/jsonrpc
ARIA2_RPC_SECRET=

# =============================================================================
# 系统配置
# =============================================================================
AGGREGATION_WINDOW_MINUTES=5
BATCH_TASK_THRESHOLD=10
IDLE_DESTROY_MINUTES=15
DOWNLOAD_BASE_PATH=../downloads

# =============================================================================
# 数据库配置 (本地路径)
# =============================================================================
DATABASE_PATH=../data/swipe.db

# =============================================================================
# 预览图路径 (本地路径)
# =============================================================================
PREVIEWS_PATH=../data/previews
